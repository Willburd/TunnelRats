            <kind>1</kind>
            <string>/// Gravity update!
if(loadedCheck)
{
    // calculate falling speed
    Zmomentum += scr_DTscale(global._gravity);
    
    // find lowest point for shadow, and as a ground check for falling!
    attemptedGround = z+512;

    
    // apply falling speed
    z += scr_DTscale(Zmomentum);
    
    
    // check floor collisions
    var getQ = floor((x - ActiveChunk.x)/16);
    var getC = floor((y - ActiveChunk.y)/16);
    if(ActiveChunk.collisionData[# getQ,getC] &gt; 0)
    {
        attemptedGround = 0;
    }
    
    // if falling, check if we've reached under our shadow position!
    if(Zmomentum &gt;= 0)
    {
        // lock to ground
        var zbeforelock = z;
        z = min(z,attemptedGround); 
        
        if(zbeforelock &lt; z)
        {
            // land on ground event
            event_user(5);
        }
        
        // ground control to major tom!
        if(z == attemptedGround)
        {
            // stop
            Zmomentum = 0;
        }
    }
}
</string>
